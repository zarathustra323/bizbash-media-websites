import { getAsArray, getAsObject } from "@parameter1/base-cms-object-path";

$ const { GAM } = out.global;

$ const {
  adIndex,
  adName,
  aliases,
  nativeX,
} = input;
$ const nodes = getAsArray(input, "nodes");

<if(nodes.length)>
  <global-content-card-deck-flow
    nodes=nodes
    aliases=aliases
    native-x=nativeX
    ad-index=adIndex
    ad-name=adName
  />
</if>
