{
  "taglib-imports": [
    "./content-page/marko.json"
  ],
  "<global-primary-image-block>": {
    "template": "./primary-image.marko"
  },
  "<global-content-media-block>": {
    "template": "./content-media.marko"
  },
  "<global-section-cards-block>": {
    "template": "./section-cards.marko",
    "@alias": {
      "type": "string",
      "required": true
    },
    "@display-image": {
      "type": "boolean",
      "default-value": true
    }
  },
  "<global-related-content-block>": {
    "template": "./related-content.marko",
    "<native-x>": {
      "@index": "number",
      "@name": "string",
      "@aliases": "array"
    },
    "@id": "number",
    "@type": "string",
    "@section-id": "number",
    "@section-name": "string",
    "@query-fragment": "object"
  },
  "<global-content-load-more-block>": {
    "template": "./content-load-more.marko",
    "<native-x>": {
      "@index": "number",
      "@name": "string",
      "@aliases": "array"
    },
    "@adIndex" : "number",
    "@adName" : "string",
    "@id": "number",
    "@type": "string",
    "@name": "string",
    "@aliases": "array",
    "@primary-section-alias": "string",
    "@section-id": "number",
    "@section-name": "string",
    "@section-skip": "number"
  },
  "<global-magazine-publications-block>": {
    "template": "./magazine-publications.marko"
  }
}
